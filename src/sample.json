{
    "$query": {
        "$from": {
            "$select": "*",
            "[users]": {
                "$filter": {
                    "$or": {
                        "[name]": [
                            "eq",
                            "Ali"
                        ],
                        "$and": {
                            "[age]": [
                                ">",
                                10
                            ]
                        }
                    }
                }
            }
        },
        "$orderby": {
            "[name]": 1
        },
        "$skip": 0,
        "$take": 10
    }
}